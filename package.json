  "lint-staged": {
    "{,src/**/}*.{md,json,ts,tsx,css,scss,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,js,html,ts,tsx,css,scss,yml}\"",
    "cleanup": "rimraf build/resources/main/static/",
    "webpack:dev": "yarn run webpack-dev-server --config webpack/webpack.dev.js --inline --port=9090 --env.stats=minimal",
    "webpack:build:main": "yarn run webpack --config webpack/webpack.dev.js --env.stats=minimal",
    "webpack:prod:main": "yarn run webpack --config webpack/webpack.prod.js  --profile",
    "start": "yarn run webpack:dev",
    "build:dev": "yarn run cleanup && yarn run webpack:build:main",
    "build": "yarn run cleanup && yarn run webpack:prod:main",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config src/test/javascript/jest.conf.js",
    "jest:update": "yarn run jest --updateSnapshot",
    "test": "yarn run lint && yarn run jest",
    "test-ci": "yarn run lint && yarn run jest:update",
    "test:watch": "yarn test --watch",
    "lint": "tslint --project tsconfig.json -e 'node_modules/**'"
  },
  "browserslist": {
    "testing": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "staging": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "production": [
      ">0.2%",
